<%= render 'base' %>
<div class="tools">
  <%= link_to '修改', edit_admin_feedback_path(@feedback), :class => "btn btn-small btn-success" %>
  <%= link_to '返回', admin_feedbacks_path, :class => "btn btn-small" %>
</div>
<div id="admin_feedback">
    <p>
    <b>Name:</b>
    <%= @feedback.name %>
  </p>
    <p>
    <b>Tel:</b>
    <%= @feedback.tel %>
  </p>
    <p>
    <b>Qq:</b>
    <%= @feedback.qq %>
  </p>
    <p>
    <b>Title:</b>
    <%= @feedback.title %>
  </p>
    <p>
    <b>Content:</b>
    <%= @feedback.content %>
  </p>
  </div>